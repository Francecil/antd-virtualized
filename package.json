    "postbuild": "node packCss.js clear",
    "deploy": "father doc deploy"
  },
  "peerDependencies": {
    "antd": "^3.19.1",
    "react": "16.x"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.8",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-window": "^1.8.0",
    "antd": "^3.19.1",
    "babel-plugin-import": "^1.11.2",
    "css-loader": "^3.0.0",
    "father": "^2.2.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "typescript": "^3.3.3",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-window": "^1.8.2"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ]
